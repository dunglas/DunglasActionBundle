{
  "name": "dunglas/action-bundle",
  "type": "symfony-bundle",
  "description": "Symfony controllers, redesigned",
  "keywords": ["Symfony", "controllers", "action", "autowiring", "routing"],
  "homepage": "https://dunglas.fr/2016/01/dunglasactionbundle-symfony-controllers-redesigned/",
  "license": "MIT",
  "authors": [
    {
      "name": "KÃ©vin Dunglas",
      "email": "dunglas@gmail.com",
      "homepage": "https://dunglas.fr"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "symfony/config": "^2.8 || ^3.0",
    "symfony/dependency-injection": "^2.8 || ^3.0",
    "symfony/finder": "^2.8 || ^3.0",
    "symfony/http-kernel": "^2.8 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.6 || ^5.0",
    "sensio/framework-extra-bundle": "^3.0.10",
    "symfony/browser-kit": "^2.8 || ^3.0",
    "symfony/console": "^2.8 || ^3.0",
    "symfony/css-selector": "^2.8 || ^3.0",
    "symfony/expression-language": "^2.8 || ^3.0",
    "symfony/framework-bundle": "^2.8 || ^3.0",
    "twig/twig": "^1.28 || ^2.0"
  },
  "conflict": {
    "symfony/framework-bundle": "<2.8.5 || >3.0,<3.0.5"
  },
  "autoload": {
    "psr-4": { "Dunglas\\ActionBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
